<h2> Forms</h2>

<table id="Form_table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Year</th>
    </tr>
  </thead>
  <tbody>

<% @forms.each do |t| %>
  <tr><td><%= link_to (t.name), form_path(t.id) %> </td><td>  <%= t.year %> </td></tr>
<% end %>
</tbody>
</table>

<p><%= link_to 'create new form', new_form_path %></p>
